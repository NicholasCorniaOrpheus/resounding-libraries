 publisher = {Conservatorium van Amsterdam},
 title = {Posthuman Creativity Labs: Artificial Intelligence and Blockchain in Music (Master's Elective Course)},
 type = {non-written output},
 url = {https://www.conservatoriumvanamsterdam.nl/en/study/classical-music/study-programmes/masters-degree-programme/electives/posthuman-creativity-labs-artificial-intelligence-and-blockchain-in-music/},
 year = {2023}
